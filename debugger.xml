<application>
  <component name="XDebuggerSettings">
    <data-views>
      <sort-values>true</sort-values>
      <option name="valueLookupDelay" value="150" />
    </data-views>
    <general>
      <evaluation-dialog-mode>CODE_FRAGMENT</evaluation-dialog-mode>
    </general>
    <debuggers>
      <debugger id="dotnet_debugger">
        <configuration>
          <option name="evaluationTimeoutMs" value="60000" />
          <option name="needNotifyWhenStoppedInExternalCode" value="false" />
          <option name="showDebuggerEventsInOutput" value="false" />
          <option name="showRunToPanel" value="false" />
          <option name="userUnhandledExceptionsIgnoreList">
            <list>
              <option value="System.OperationCanceledException" />
              <option value="System.Threading.Tasks.TaskCanceledException" />
              <option value="System.Threading.ThreadAbortException" />
              <option value="System.AggregateException" />
              <option value="System.Web.Services.Protocols.SoapException" />
              <option value="System.NullReferenceException" />
              <option value="System.InvalidOperationException" />
              <option value="System.ArgumentException" />
            </list>
          </option>
        </configuration>
      </debugger>
      <debugger id="javascript">
        <configuration>
          <custom-object-presentation />
        </configuration>
      </debugger>
      <debugger id="oracle">
        <configuration>
          <option name="steppingPauseAtBegin" value="true" />
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>