<application>
  <component name="CodeInsightSettings">
    <option name="selectAutopopupSuggestionsByChars" value="true" />
    <option name="AUTO_POPUP_JAVADOC_INFO" value="true" />
    <option name="COMPLETION_CASE_SENSITIVE" value="2" />
    <option name="SELECT_AUTOPOPUP_SUGGESTIONS_BY_CHARS" value="true" />
    <option name="AUTOCOMPLETE_ON_CODE_COMPLETION" value="false" />
    <option name="INSERT_BRACE_ON_ENTER" value="false" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Consolas" />
  </component>
  <component name="EditorSettings">
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_LEADING_WHITESPACES_SHOWN" value="false" />
    <option name="IS_INNER_WHITESPACES_SHOWN" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="JSEditorOptions">
    <option name="ESCAPE_PASTED_TEXT" value="false" />
    <option name="CONVERT_HTML_TO_JSX" value="false" />
  </component>
  <component name="JsonEditorOptions">
    <option name="COMMA_ON_ENTER" value="false" />
    <option name="COMMA_ON_MATCHING_BRACES" value="false" />
    <option name="COMMA_ON_PASTE" value="false" />
    <option name="AUTO_QUOTE_PROP_NAME" value="false" />
    <option name="AUTO_WHITESPACE_AFTER_COLON" value="false" />
    <option name="ESCAPE_PASTED_TEXT" value="false" />
  </component>
  <component name="SqlEditorOptions">
    <option name="aliasQualification" value="Always" />
    <option name="namespaceQualification" value="Always" />
    <option name="tableQualification" value="Always" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\bNotImplementedException\b" />
    <pattern useCustomColors="true" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;TODO)(\W|$)(.*)">
      <option name="FOREGROUND" value="a8c023" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="a8c023" />
    </pattern>
    <pattern pattern="(?&lt;=\W|^)(?&lt;TAG&gt;BUG)(\W|$)(.*)" />
    <pattern useCustomColors="true" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;PERFORMANCE)(\W|$)(.*)">
      <option name="FOREGROUND" value="c0bd" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="c0bd" />
    </pattern>
  </component>
  <component name="YamlEditorOptions">
    <option name="useSmartPaste" value="false" />
  </component>
</application>